#!/usr/bin/env bash

mkdir tests temp log

for d in {3762..3769}; do
	mkdir -p $d
	/usr/bin/time -v hops2uvfits.py     \
	        --recomputeblfits --clean --recomputeuv --fixsrcname \
	        $SRCDIR/$d $d      \
		>  log/uvfits-$d.log        \
		2> log/uvfits-$d.err        &
done

wait $(jobs -p)
