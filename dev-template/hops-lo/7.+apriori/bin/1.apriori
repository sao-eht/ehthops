#!/usr/bin/env bash

mkdir tests temp log
for d in {3597..3601}; do
	mkdir -p $d
	/usr/bin/time -v                   \
	        cal_apriori_pang_uvfits.py \
	        --caldir /sefd/$d          \
	        --outdir $d                \
	        /data/$d                   \
		>  log/apriori-$d.log      \
		2> log/apriori-$d.err      &
done
wait $(jobs -p)
